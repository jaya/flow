<h1>Show Status</h1>

<ul>

  <li>
    <strong>Name:</strong>
    <%= @status.name %>
  </li>

  <li>
    <strong>Description:</strong>
    <%= @status.description %>
  </li>

  <li>
    <strong>Order:</strong>
    <%= @status.order %>
  </li>

  <li>
    <strong>Enable:</strong>
    <%= @status.enable %>
  </li>

</ul>

<span><%= link "Edit", to: Routes.status_path(@conn, :edit, @status) %></span>
<span><%= link "Back", to: Routes.status_path(@conn, :index) %></span>
